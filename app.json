{
  "expo": {
    "name": "StockPay",
    "slug": "StockPay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "primaryColor": "#E1AE3C",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIndentifier": "com.anihpatrickugo.StockPay",
      "supportsTablet": true,
      "infoPlist": {
       "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.77067681655-ct1u80q6b9siphg7t2o4vt18dbun5qlp"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anihpatrickugo.StockPay"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "191da65b-0455-443c-9801-4b43d3857a94"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],

      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/roboto/Roboto_500Medium.ttf"
          ]
        }
      ]
    ]
  }
}
